<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.attafakkur.consumergithub">

    <uses-permission android:name="com.attafakkur.githubuserapp.READ_DATABASE" />
    <uses-permission android:name="com.attafakkur.githubuserapp.WRITE_DATABASE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GithubUserApp">
        <activity android:name=".view.activity.FavoriteActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.UserDetailActivity" />
    </application>

</manifest>